{
  "name": "zyndex-server",
  "version": "0.2.0-8",
  "description": "Backend Server for Handling Zyndex Frontend",
  "main": "app.js",
  "scripts": {
    "prepare": "husky install && husky add .husky/commit-msg \"npx --no-install commitlint --edit \"$1\"\"",
    "stage": "git add",
    "commit": "cz",
    "prerelease": "standard-version --prerelease --sign",
    "patchrelease": "standard-version --release-as patch --sign",
    "minorrelease": "standard-version --release-as minor --sign",
    "majorrelease": "standard-version --release-as major --sign",
    "push": "git push --follow-tags origin dev",
    "test": "set NODE_ENV=test && ts-mocha --paths tests/**/*.spec.ts -r 'tests/require' --reporter mocha-simple-html-reporter --reporter-options output=test-report.html",
    "stest": "set NODE_ENV=test && ts-mocha --paths -r 'tests/require'",
    "dev": "set NODE_ENV=development && nodemon --ignore tests/ -r tsconfig-paths/register app.ts",
    "build": "set NODE_ENV=production && tsc && tsc-alias",
    "serve": "set NODE_ENV=production && cd dist && npm serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zyndex-drive/server.git"
  },
  "keywords": [
    "Nodejs",
    "Backend",
    "Google",
    "Drive",
    "mongodb",
    "oauth2"
  ],
  "author": "tks18",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zyndex-drive/server/issues"
  },
  "homepage": "https://github.com/zyndex-drive/server#readme",
  "dependencies": {
    "@godaddy/terminus": "^4.9.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "crypto-js": "^4.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.1.0",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "mongoose": "^5.12.15",
    "nanoid": "^3.1.23",
    "nodemailer": "^6.6.2",
    "query-string": "^7.0.1",
    "sha1": "^1.1.1",
    "x-xss-protection": "^2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.2.21",
    "@types/chai-as-promised": "^7.1.4",
    "@types/crypto-js": "^4.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/expect": "^24.3.0",
    "@types/express": "^4.17.12",
    "@types/http-server": "^0.12.1",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/md5": "^2.3.0",
    "@types/mocha": "^8.2.3",
    "@types/node": "^15.12.4",
    "@types/nodemailer": "^6.4.2",
    "@types/sha1": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.29.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-nodejs": "^2.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^7.0.1",
    "jsdoc": "^3.6.7",
    "mocha": "^9.0.2",
    "mocha-simple-html-reporter": "^2.0.0",
    "mocha-steps": "^1.3.0",
    "standard-version": "^9.3.1",
    "ts-mocha": "^8.0.0",
    "tsc-alias": "^1.3.7",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
